@model IEnumerable<ResultServiceDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}
<h2>Personeller</h2>
<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col"></th>
            <th scope="col">Hizmet Başlığı</th>
            <th scope="col">Açıklama</th>
            <th scope="col">Sil</th>
            <th scope="col">Güncelle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ServiceId</td>
                <td>@item.Title</td>
                <td>@item.Description</td>
                <td><a href="/Service/Delete/@item.ServiceId" class="btn mb-1 btn-rounded btn-outline-danger">Sil</a></td>
                <td><a href="/Service/Update/@item.ServiceId" class="btn mb-1 btn-rounded btn-outline-success">Güncelle</a></td>
            </tr>
        }


    </tbody>
</table>
<a href="/Service/Add/" class="btn btn-info">Yeni Hizmet Girişi</a>